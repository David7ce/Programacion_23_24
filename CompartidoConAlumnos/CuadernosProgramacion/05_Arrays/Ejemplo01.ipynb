    "const int TAM = 20;\n",
    "int[] tabla = new int[TAM];\n",
    "int suma=0;\n",
    "int i;\n",
    "for(i=1 ; i <= TAM ; i++ ) {\n",
    "    suma = suma + i;\n",
    "    tabla[i-1]=suma;\n",
    "}\n",
    "for(i=0 ; i < TAM ; i++ ) {\n",
    "    Console.WriteLine(\"El valor de tabla[{0}] es: {1}\",i,tabla[i]);\n",
    "}\n",
    "Console.WriteLine(\"FIN\");    "
